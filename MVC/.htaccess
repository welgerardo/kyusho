ErrorDocument 404 /index.php
#DirectoryIndex index.php
<IfModule mod_rewrite.c>
  	RewriteEngine on
        
        RewriteRule ^logs/(olog)$ index.php?folder=forms&control=LoginForms&method=mainLoginForm [L]
        RewriteRule ^logs/(oaccount)$ index.php?folder=forms&control=NewAccounts&method=newAccountForm  [L]
        RewriteRule ^logs/(ocreate)$ index.php?folder=forms&control=NewAccounts&method=createNewAccount  [L]        
        RewriteRule ^logs/(ologin)$ index.php?folder=forms&control=LoginForms&method=userLogin&mode=ALL [L]
        RewriteRule ^logs/(clogin)$ index.php?folder=forms&control=LoginForms&method=userLogin&mode=CHECK [L]


        RewriteRule ^(bebe|menino|menina)$ index.php?folder=pages&control=ProductsPage&cat=$1 [L,QSA]
        RewriteRule ^pd/(.*)/(.*)$ index.php?folder=pages&control=ProductPage&toke=$1 [L,QSA]
        RewriteRule ^shopcart$ index.php?folder=pages&control=ShoppingCartPage [L]
        

        RewriteRule ^(checkoutEditInvoice)$ index.php?folder=forms&control=InvoiceDataForm&method=editInvoiceData [L]
        RewriteRule ^(checkoutEditShipping)$ index.php?folder=pages&control=CheckoutPage&method=getEditShipping [L]
        RewriteRule ^(shipping-checkout)$ index.php?folder=forms&control=ShippingAddressDataForm&method=checkoutSaveShippingAdress [L]
        RewriteRule ^(pay-options)$ index.php?folder=pages&control=CheckoutPage&method=setPaymentOptions [L]

        RewriteRule ^add-fav$ index.php?folder=favorites&control=CFavorites&method=addFavorites [L]
        RewriteRule ^rem-fav$ index.php?folder=favorites&control=CFavorites&method=delFavorites [L]

        RewriteRule ^(order-confirmation)$ index.php?folder=pages&control=OrderConfirmation [L]

        RewriteRule ^(shippingAddress)$ index.php?folder=pages&control=CheckoutPage&method=setAddressId [L]

        RewriteRule ^(invoice-form|invoice-checkout)$ index.php?folder=forms&control=InvoiceDataForm&method=saveInvoiceData [L]

        RewriteRule ^(shipping-form/remove)$ index.php?folder=pages&control=ClientAreaShipping&method=removeShippingAddress [L]
        RewriteRule ^(shipping-form)$ index.php?folder=pages&control=ClientAreaShipping&method=saveShippingAdress [L]
        RewriteRule ^(newshipping)$ index.php?folder=pages&control=ClientAreaShipping&method=blankShippingForm [L]
        
        RewriteRule ^(showcart)$ index.php?folder=shopping-cart&control=ShoppingCart&method=getCartItens [L]
        RewriteRule ^(addcart)$ index.php?folder=shopping-cart&control=ShoppingCart&method=productId [L]

        RewriteRule ^sendmessplus$ index.php?folder=forms&control=itemForms&method=sendMessPlus [L]


        RewriteRule ^curso/([0-9]+)/(.*)$ index.php?folder=pages&control=workshopPage&toke=$1 [L]
        RewriteRule ^cursos$ index.php?folder=pages&control=workshopsPage [L]

 	RewriteCond %{REQUEST_FILENAME} !-f
  	RewriteCond %{REQUEST_FILENAME} !-d
        RewriteRule ^(.*)$ index.php?folder=pages&control=home&page=$1 [L,QSA]

  	
</IfModule>

